{
  "author": "Dean Merchant",
  "dependencies": {
    "jest-matcher-utils": "23.6.0"
  },
  "description": "A jest matcher to ensure a react component will render",
  "devDependencies": {
    "@types/enzyme": "3.1.14",
    "@types/jest": "23.3.3",
    "@types/jest-get-type": "21.0.0",
    "@types/jest-matcher-utils": "21.0.1",
    "@types/lodash": "4.14.116",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.8",
    "codecov": "3.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "jest": "23.6.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.1"
  },
  "peerDependencies": {
    "enzyme": "3.x",
    "jest": "22.x",
    "react": "16.x"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/common-types.ts",
      "<rootDir>/src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "./setupTestFramework.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.tsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "jest-to-render-without-error",
  "repository": "git@github.com:Dean177/jest-to-render-without-error.git",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --noEmit --watch",
    "lint": "tslint ./src/**/*.ts",
    "prepublish": "yarn build && yarn lint && yarn test",
    "report-coverage": "codecov",
    "test": "jest"
  },
  "version": "0.2.0"
}
